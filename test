  const changeTableData = (id) => {
    setCheckedRows((current) => {
      const currentIndex = current.indexOf(id);
      if (currentIndex === -1) {
        return [...current, id];
      } else {
        const updatedRows = [...current];
        updatedRows.splice(currentIndex, 1);
        return updatedRows;
      }
    });
  };
